<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style>
		.urlTextBox {
			width: 100%;
		}
		
		.webPageFrame {
			display: block;
			width: 100%;
			height: 100%;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:TextBox ui:field="urlTextBox" styleName="{style.urlTextBox}" />
		<g:Frame ui:field="webPageFrame" styleName="{style.webPageFrame}" />
	</g:HTMLPanel>
</ui:UiBinder>